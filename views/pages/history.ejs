                        datevalue1 = datevalue.toDateString();
                            let dig = ["January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December"];
                        let dig2 = ["Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"];
                        datevalue1 = datevalue1.split(" ");
                        let month = dig[dig2.indexOf(datevalue1[1])]

                        let answer = month + "/" + datevalue1[2] + "/" + datevalue1[3]
                        return {
                            name: "Present",
                            badge: answer, // Event badge (optional)
                            date: answer, // Date range
                            description: "Attendence is Marked.", // Event description (optional)
                            type: "event",
                            color: "#63d867" // Event custom color (optional)
                        }
                    }
                    rows.data.map((ele) => {
                        val.push(changer(ele.date))
                    })
                    console.log(val)
                </script>

